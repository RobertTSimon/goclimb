<%= simple_form_for([ @route, @review ], remote: true) do |form| %>
  <%= form.input :title, as: :string %>
  <%= form.input :description, as: :text %>
  <%= form.input :rating, as: :integer %>
  <%= form.select :alert, Route::ALERTS %>
  <%= form.button :submit, class: "btn btn-success" %>
<% end %>
