<%= render 'shared/navbar' %>
<div class="card-review container">
  <p><%= review.title %> from <%= review.user.username %></p>
  <p><%= review.description %></p>
  <p><%= review.rating %></p>

  <%= link_to "Destroy", review_path(review), method: "delete", data: { confirm: "Do you want to delete this review?"}, class: "btn btn-primary" %>
</div>

